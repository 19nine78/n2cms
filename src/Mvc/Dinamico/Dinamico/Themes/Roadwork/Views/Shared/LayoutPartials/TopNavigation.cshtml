@using (Content.BeginScope(Request["returnUrl"], reallyBeginScope: Content.Current.Page == null))
{
<ul id="menu" class="topmenu menu clearfix level1">
	<li class="home">
		<a href="@Content.Traverse.StartPage.Url" title="@Content.Traverse.StartPage.Title"><img src="@Url.ThemedContent("~/Content/home.png")" alt="@Content.Traverse.StartPage.Title" /></a>
	</li>
	@Html.Tree(parallelRoot: false, takeLevels: 3).ExcludeRoot(true)
</ul>
}
<script type='text/javascript'>//<![CDATA[
	$("#menu").menudown({ submenuclass: "submenu menu clearfix level2" });
//]]></script>
