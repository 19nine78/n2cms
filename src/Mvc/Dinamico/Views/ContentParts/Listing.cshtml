@model Dinamico.Models.ContentPart
@using N2.Web.Mvc.Html
		   
@{ Content.Define(ie => 
	   {
		   ie.Title = "Listing";
		   ie.Link("ListFrom");
	   }); }


<div class="part teaser">
	@Html.Loop(Content.Traverse.Children((N2.ContentItem)Content.Data.ListFrom),
		template: @<div class="item">@Html.Partial("Partials/ContentLink", item.Data, new ViewDataDictionary { {"preferredSize", "thumb"} })</div>,
		wrapper:@<div class="list">@item.RenderContents()</div>)
</div>